{
    "store.custom#soldaduras-y-generadores-refacciones":{
        "blocks":[
            "list-context.image-list#full-banner",
			"flex-layout.row#sgr-row-title",
			"flex-layout.row#sgr-banners",
			"flex-layout.row#sgr-row-title-2",
			"shelf#sgr-recommended-products"
        ]
    },
	"flex-layout.row#sgr-row-title": {
	  "children": [
	    "rich-text#sgr-title"
	  ]
	},
	"flex-layout.row#sgr-row-title-2": {
	  "children": [
	    "rich-text#sgr-title"
	  ]
	},
	"flex-layout.row#sgr-row": {
	  "children": [
	    "slider-layout#sgr-slider"
	  ]
	},
	"slider-layout#sgr-slider": {
		"props": {
		  "itemsPerPage": {
		    "desktop": 4,
		    "tablet": 3,
		    "phone": 2
		  },
		  "infinite": true,
		  "showNavigationArrows": "desktopOnly",
		  "blockClass": "carousel"
		},
		"children":[
			"image#sgr-banner-1","image#sgr-banner-2","image#sgr-banner-3","image#sgr-banner-4","image#sgr-banner-5","image#sgr-banner-6","image#sgr-banner-7"
			]
	},
    "rich-text#sgr-title":{
      "props": {
        "textAlignment": "Left",
        "textPosition": "Left",
        "text": "**Título 1**",
        "textColor": "muted-1",
        "font": "t-heading-5",
        "blockClass": "title-01"
      }
    },
    "rich-text#sgr-title-2":{
      "props": {
        "textAlignment": "Left",
        "textPosition": "Left",
        "text": "**Título 2**",
        "textColor": "muted-1",
        "font": "t-heading-5",
        "blockClass": "title-01"
      }
    },
